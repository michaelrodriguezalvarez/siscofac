{% extends 'base_escritorio.html.twig' %}

{% block title %}{% trans %}Modificar{% endtrans %} {% trans %}Contrato{% endtrans %}{% endblock %}

{% block  header %}
    {% trans %}Modificar{% endtrans %} {% trans %}Contrato{% endtrans %}
    <span class="text-info">{% trans %}Anno{% endtrans %}  {{ anno_actual }} ({{ cantidad_de_contratos_del_anno_actual }})</span>
{% endblock %}

{% block  breadscrum %}
    <div class="breadcrumb-item">{% trans %}Contrato{% endtrans %}</div>
    <div class="breadcrumb-item">{% trans %}Modificar{% endtrans %} {% trans %}Contrato{% endtrans %}</div>
{% endblock %}


{% block body %}
    <div class="row">
    <div class="form-control">

        <div class="container">
            <div class="row">
                {% include 'contrato/botones_adicionar.html.twig' %}
                {% include 'contrato/botones_contrato.html.twig' %}
            </div>
        </div>

        {% include 'contrato/ventana_modal.html.twig' %}

        {% for message in app.flashes('notice') %}
            <div class="alert-info">
                {{ message |trans}}
            </div>
        {% endfor %}

        <br>

        {{ include ('contrato/formulario.html.twig') }}
        {#{ include('contrato/_delete_form.html.twig') }#}
    </div>
    </div>


{% endblock %}

{% block javascripts %}
    <script src="{{ asset('siscofac/vendors/components/jquery/jquery.min.js') }}"></script>
    <script src="{{ asset('build/contrato.js') }}"></script>
{% endblock %}