{% extends 'base.html.twig' %}

{% block title %}Suplemento index{% endblock %}

{% block body %}
    <h1>Suplemento index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Contrato</th>
                <th>Numero</th>
                <th>Objeto</th>
                <th>ValorSuplementoCup</th>
                <th>ValorSuplementoCuc</th>
                <th>FechaInicio</th>
                <th>FechaTerminacion</th>
                <th>Numero Acuerdo</th>
                <th>Fecha Acuerdo</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for suplemento in suplementos %}
            <tr>
                <td>{{ suplemento.id }}</td>
                <td>{{ suplemento.contrato }}</td>
                <td>{{ suplemento.numero }}</td>
                <td>{{ suplemento.objeto }}</td>
                <td>{{ suplemento.valorSuplementoCup }}</td>
                <td>{{ suplemento.valorSuplementoCuc }}</td>
                <td>{{ suplemento.fechaInicio ? suplemento.fechaInicio|date('d/m/Y') : '' }}</td>
                <td>{{ suplemento.fechaTerminacion ? suplemento.fechaTerminacion|date('d/m/Y') : '' }}</td>
                <td>{{ suplemento.numeroAcuerdo }}</td>
                <td>{{ suplemento.fechaAcuerdo ? suplemento.fechaAcuerdo|date('d/m/Y') : '' }}</td>
                <td>
                    <a href="{{ path('suplemento_show', {'id': suplemento.id}) }}">show</a>
                    <a href="{{ path('suplemento_edit', {'id': suplemento.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="10">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('suplemento_new') }}">Create new</a>
{% endblock %}